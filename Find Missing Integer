var arr=[1,2,3,5,6,7,8,9]

function missingnumber(arr){
    var temp=[]

    for(var i=1;i<=arr.length;i++){
        temp[i]=0;
    }

    for(var i=0;i<arr.length;i++){
        temp[arr[i]]=1;
    }
    for(var i=1;i<=arr.length;i++){
        if(temp[i]==0){
            return i;
        }
    }
}

console.log(missingnumber(arr))
